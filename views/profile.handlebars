<div class="container">
    <div class="row">
        <div class="col-md-6">
            <div class="profile-pic">
                <input type="image" src="" alt="Submit" width="48" height="48">
            </div>
            <div class="contact-info">
                <h3>Contact info</h3>
                <p>Github - clickable icon link</p>
                <p>Slack - clickable icon link</p>
                <p>Linked-in - clickable icon link</p>
                <p>email - clickable icon link</p>
            </div>
        </div>
        <div class="col-md-6">
            <div class="bio">
                <h3>BIO</h3>
                <p>{{users.[0].bio}}</p>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <p>JavaScript</p>
            <p>Heroku</p>
            <p>Mysql</p>
        </div>
        <div class="col-md-6">
            <h3>Level of skill</h3>
            <form>
                <label for="javascript" class="form-label">JavaScript</label>
                <input type="range" class="form-range" min="0" max="5" id="javascript">
                <label for="heroku" class="form-label">Heroku</label>
                <input type="range" class="form-range" min="0" max="5" id="heroku">
                <label for="mysql" class="form-label">Mysql</label>
                <input type="range" class="form-range" min="0" max="5" id="mysql">
            </form>
        </div>
    </div>
</div>
